@using Folkways.Collection.Models
@{
    Style.Require("Folkways");
    var albumPart = (AlbumPart)Model.ContentPart;
}

@if (albumPart.ImageGallery != null) {

            @T(albumPart.ImageGallery)

}
else {

      <img alt="@Model.ContentItem.TitlePart.Title" src="@Url.Content("http://media.smithsonianfolkways.org/images/album_covers/SF700/" + albumPart.CatalogNumber + ".jpg")" />

}
